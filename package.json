    "@vuepress/plugin-shiki": "2.0.0-rc.21",
    "@vuepress/theme-default": "2.0.0-rc.21",
    "@vuepress/utils": "2.0.0-rc.9",
    "eslint": "^8.57.0",
    "eslint-plugin-markdownlint": "^0.5.0",
    "eslint-plugin-perfectionist": "^2.7.0",
